{
    "name": "ts-react-starter",
    "version": "1.0.0",
    "description": "typescript, react, webpack, parcel",
    "scripts": {
        "analyze": "npm run build --report",
        "lint": "tslint --type-check --project ./tsconfig.json",
        "clean": "rimraf dist && rimraf .cache-loader && rimraf package-lock.json && rimraf node_modules",
        "dev": "cross-env APP_ENV=dev webpack-dev-server --config build/webpack.config.js --mode development --open --hot",
        "dev:parcel": "parcel serve src/index.html",
        "build": "webpack --config build/webpack.config.js --mode production",
        "build:qa": "cross-env APP_ENV=qa npm run build",
        "build:prod": "cross-env APP_ENV=prod npm run build",
        "build:parcel": "cross-env NODE_ENV=production parcel build src/index.html --public-url .",
        "watch": "parcel watch src/index.html"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run lint"
        }
    },
    "lint-staged": {
        "*.{scss,css,ts,tsx}": [
            "prettier --write 'src/**/*.{js,jsx,scss,css,ts,tsx}'",
            "git add"
        ]
    },
    "keywords": [
        "webpack",
        "typescript",
        "admin",
        "react",
        "starter-template"
    ],
    "author": "ChengZhenghao",
    "dependencies": {
        "antd": "^3.10.9",
        "axios": "^0.18.0",
        "babel-polyfill": "^6.26.0",
        "lodash": "^4.17.11",
        "path-to-regexp": "^2.4.0",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-hot-loader": "^4.3.12",
        "react-loadable": "^5.5.0",
        "react-router-dom": "^4.2.2"
    },
    "devDependencies": {
        "@babel/core": "^7.1.6",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.6",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@svgr/webpack": "^4.1.0",
        "@types/lodash": "^4.14.119",
        "@types/moment": "^2.13.0",
        "@types/node": "^10.12.12",
        "@types/react": "^16.7.13",
        "@types/react-dom": "^16.0.11",
        "@types/react-loadable": "^5.4.2",
        "@types/react-router-dom": "^4.3.1",
        "@types/webpack-env": "^1.13.5",
        "autoprefixer": "^9.3.1",
        "babel-loader": "^8.0.4",
        "babel-plugin-import": "^1.11.0",
        "babel-plugin-module-resolver": "^3.1.1",
        "cache-loader": "^1.2.5",
        "case-sensitive-paths-webpack-plugin": "^2.1.2",
        "cross-env": "^5.1.4",
        "css-loader": "^1.0.1",
        "customaddcomponents": "^1.0.35",
        "file-loader": "^2.0.0",
        "fs-extra": "^7.0.1",
        "html-webpack-plugin": "^3.0.6",
        "husky": "^1.2.0",
        "less": "^2.7.2",
        "less-loader": "^4.1.0",
        "lint-staged": "^8.1.0",
        "mini-css-extract-plugin": "^0.4.5",
        "moment-locales-webpack-plugin": "^1.0.7",
        "node-sass": "^4.10.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "parcel-bundler": "^1.10.3",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.15.3",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.1.0",
        "shortid": "^2.2.14",
        "style-loader": "^0.23.1",
        "stylelint": "^9.9.0",
        "stylelint-config-css-modules": "^1.2.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-order": "^2.0.0",
        "terser-webpack-plugin": "^1.1.0",
        "thread-loader": "^1.2.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslib": "^1.9.0",
        "tslint": "^5.11.0",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-react": "^3.5.1",
        "typescript": "^3.1.6",
        "typings-for-css-modules-loader": "^1.7.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.26.1",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "webpack-hot-middleware": "^2.24.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 11"
    ]
}
